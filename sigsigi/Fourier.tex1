\subsubsection{Linearit"at}\index{Linearitat@{Linearit\"at}}
\myboxx{
\begin{equation}
  \alpha\cdot f(t) + \beta\cdot g(t) \FT \alpha\cdot F(j\omega) + \beta\cdot G(j\omega)
\end{equation}}
\subsubsection{Parseval's Theorem\index{Theorem!Parseval's}}
\myboxx{
\begin{equation}
  \int\limits_{-\infty}^{\infty}f(t)g^{\ast}(t)dt = \frac{1}{2\pi}
  \int\limits_{-\infty}^{\infty}F(j\omega)G^{\ast}(j\omega)d\omega\label{FOU_FORM_PARSE}
\end{equation}}
\subsubsection{Bessel's Theorem\index{Theorem!Bessel's} (Rayleigh's Theorem\index{Theorem!Rayleigh's } \cite{ZIE:TRA:76})}
\myboxx{
\begin{equation}
  \int\limits_{-\infty}^{\infty}|f(t)|^2 dt = \frac{1}{2\pi}
  \int\limits_{-\infty}^{\infty}|F(j\omega)|^2 d\omega\label{equ:FOU:3}
\end{equation}}
\bsp{Bessel's Theorem\index{Theorem!Bessel's} (Rayleigh's Theorem\index{Theorem!Rayleigh's })}
Gesucht ist $\int\limits_{-\infty}^{\infty}\left | \frac{\sin(\alpha\omega)}{\omega}\right |^2 d\omega$.\\ Mit $p_{\alpha}(t)\FT \frac{2}{\omega}\sin(\alpha\omega)$ folgt: $\int\limits_{-\infty}^{\infty}\left | \frac{\sin(\alpha\omega)}{\omega}\right |^2 d\omega=2\pi\cdot\int\limits_{-\infty}^{\infty}\left | \frac{p_{\alpha}(t)}{2}\right |^2 dt=\frac{\pi}{2}\int\limits_{-\alpha}^{\alpha}dt=\alpha\pi.$
\subsubsection{"Ahnlichkeit}\index{Ahnlichkeit@{\"Ahnlichkeit}}
\myboxx{
\begin{equation}
  f(\alpha t) \FT \frac{1}{|\alpha|}F \left (j\frac{\omega}{\alpha} \right ) \quad\alpha \in\mathbb{R}\setminus \{0\}\label{FRE_EQU_AEH}
\end{equation}}\\~\\
\vspace*{-8mm}\subsubsection{Verschiebung im Zeitbereich}\index{Verschiebung!im Zeitbereich}
\myboxx{
\begin{equation}
  f(t\pm t_0) \FT F(j\omega)e^{\pm j\omega t_0}
\end{equation}}\\~~\\
Beweis: 
\begin{eqnarray*}
\int\limits_{-\infty}^{\infty}f(t\pm t_0)e^{-j\omega t} dt& = & ?\\
\int\limits_{-\infty}^{\infty}f(t\pm t_0)e^{-j\omega (t\pm t_0)} dt& = & ? \cdot e^{\mp j\omega t_0} \\
\text{mit  } t'=t\pm t_0 & & -\infty \longrightarrow -\infty \\
\text{und  } \partial t'=\partial t & & +\infty \longrightarrow +\infty \\
\int\limits_{-\infty}^{\infty}f(t')e^{-j\omega t'}dt' & = & F(j\omega)=~? \cdot e^{\mp j\omega t_0} \\
&\longrightarrow & ?=F(j\omega)\cdot e^{\pm j\omega t_0}=\int\limits_{-\infty}^{\infty}f(t\pm t_0)e^{-j\omega t} dt 
\end{eqnarray*}
\subsubsection{Verschiebung im Frequenzbereich}\index{Verschiebung!im Frequenzbereich}
\myboxx{
\begin{equation}
  f(t)e^{\pm j\omega_0 t} \FT F(j(\omega\mp\omega_0))\label{FOU_FORM_VF}
\end{equation}}
\bsp{Verschiebung im Frequenzbereich}
Gesucht ist die Fourier-Transformierte von $\cos\klam{\omega_o t}$. Mit $\frac{1}{2}\FT\pi\delta(\omega)$ wird:
\begin{equation*}
  \cos\klam{\omega_0 t}=\frac{e^{j\omega_0 t}+e^{-j\omega_0 t}}{2}\FT \pi\delta(\omega-\omega_0)+\pi\delta(\omega+\omega_0)
\end{equation*}
\subsubsection{Ableitung im Zeitbereich}\index{Ableitung!im Zeitbereich}
\myboxx{
\begin{equation}
  \frac{\partial^n f(t)}{\partial t^n} \FT (j\omega)^n F(j\omega)
\end{equation}}
\newpage
\vspace*{-10mm}\bsp{Ableitung im Zeitbereich} 
Es ist $f(t)=\Lambda_{\alpha}(t)$
$(\alpha>0)$. Gesucht ist die Fourier-Transformierte von $f(t)$.
\begin{eqnarray*}
  \frac{\partial f(t)}{\partial t} &=& \frac{1}{\alpha} u(t+\alpha) - \frac{2}{\alpha} u(t) + \frac{1}{\alpha} u(t-\alpha)\\
 \frac{\partial^2 f(t)}{\partial t^2} &=& \frac{1}{\alpha} \delta(t+\alpha) - \frac{2}{\alpha} \delta(t) + \frac{1}{\alpha} \delta(t-\alpha)\\
\frac{\partial^2 f(t)}{\partial t^2} &\FT& \frac{1}{\alpha}e^{j\omega\alpha} - \frac{2}{\alpha}  + \frac{1}{\alpha} e^{-j\omega\alpha}=\frac{1}{\alpha}\klam{e^{j\omega\alpha/2}-e^{-j\omega\alpha/2}}^2=\frac{-4}{\alpha}\klam{\sin\klam{\frac{\omega\alpha}{2}}}^2\\
f(t) & \FT & \frac{1}{(j\omega)^2}\cdot\frac{-4}{\alpha}\klam{\sin\klam{\frac{\omega\alpha}{2}}}^2= \frac{4}{\alpha\omega^2}\klam{\sin\klam{\frac{\omega\alpha}{2}}}^2\\
f(t)=\Lambda_{\alpha}(t) & \FT & \alpha\cdot sinc^2\klam{\frac{\omega\alpha}{2}}
\end{eqnarray*}

\subsubsection{Ableitung im Frequenzbereich}\index{Ableitung!im Frequenzbereich}
\myboxx{
\begin{equation}
  t^n f(t) \FT j^n \frac{\partial F(j\omega)}{\partial \omega^n}
\end{equation}}
\subsubsection{Faltung im Zeitbereich (Multiplikation im Frequenzbereich)}\index{Faltung!im Zeitbereich}
\myboxx{
\begin{equation}
 f(t) \ast g(t) = \int\limits_{-\infty}^{\infty} f(\tau)g(t-\tau)d\tau \FT F(j\omega) \cdot G(j\omega)
\end{equation}}
\subsubsection{Faltung im Frequenzbereich (Multiplikation im Zeitbereich)}
\myboxx{
\begin{equation}
 f(t) \cdot g(t) \FT \frac{1}{2\pi}F(j\omega) \ast G(j\omega)
\end{equation}}
\subsubsection{Vertauschungssatz (Dualit"at)}\index{Vertauschungssatz}\index{Dualitat@ {Dualit\"at}}
\myboxx{
\begin{eqnarray}
 f(t) & \FT & F(j\omega)\nonumber \\
 F(t) & \FT & 2\pi \cdot f(-j\omega)
\end{eqnarray}}\\
\bsp{Vertauschungssatz} Gesucht ist die Fourier-Transformierte von
$\frac{1}{\alpha^2+t^2}$.\\ Mit $e^{-\alpha|t|}\FT
\frac{2\alpha}{\alpha^2+\omega^2}$ folgt $\frac{1}{\alpha^2+t^2}\FT
2\pi\cdot\frac{1}{2\alpha}e^{-\alpha|\omega|}=\frac{\pi}{\alpha}e^{-\alpha|\omega|}$.
\subsubsection{Modulation}\index{Modulation}
\myboxx{
\begin{eqnarray}
 \cos(\alpha t) \cdot f(t) & \FT & \frac{1}{2}\cdot \left [  F(j(\omega-\alpha)) + F(j(\omega+\alpha))\right ] \\
 \sin(\alpha t) \cdot f(t) & \FT &   \frac{1}{2j}\cdot \left  [ F(j(\omega-\alpha)) - F(j(\omega+\alpha))\right ]
\end{eqnarray}}\\~~\\
\matlogo~stellt die Funktionen {\tt fourier} und {\tt ifourier} (beide symbolisch) zur Verf"ugung.\index{matlab@{\matlogo}}\index{fourier@{\tt fourier}}\index{ifourier@{\tt ifourier}} 
Mit \verb¦ syms t w a real¦ und \verb¦fourier(sin(a*t))¦ erhalten wir:\\
\nit \verb¦ i*pi*(dirac(w+a)-dirac(w-a))¦.
\subsubsection{Zeitumkehrung}\index{Zeitumkehrung}
\myboxx{
\begin{equation}
 f(-t) \FT F(-j\omega)
\end{equation}}\\~\\
\nit Die Zeitumkehrungsformel folgt aus der
\"Ahnlichkeitsformel~\ref{FRE_EQU_AEH}. 
\subsubsection{Integration im Zeitbereich}\index{Integration}
\myboxx{
\begin{equation}
 \int\limits_{-\infty}^{t}f(\tau)d\tau \FT \frac{F(j\omega)}{j\omega}+F(0)\pi\delta(\omega)
\end{equation}}\\~\\
\subsubsection{Anfangswerte}\index{Anfangswert}
\myboxx{
\begin{equation}
 f(0)=\frac{1}{2\pi}\int\limits_{-\infty}^{\infty}F(j\omega)d\omega \hspace*{1cm} F(0)=\int\limits_{-\infty}^{\infty}f(t)dt 
\end{equation}}\\~\\
\subsubsection{Unendlich lange Folge von Dirac-Impulsen\index{Dirac!Impuls@{-Impuls}} im Zeit- und Frequenzbereich}
\myboxx{
\begin{equation}
 \sum_{n=-\infty}^{\infty} \delta(t-n\cdot t_0) \FT \sum_{n=-\infty}^{\infty} \frac{2\pi}{t_0}\delta\klam{\omega-n\cdot \frac{2\pi}{t_0}}
\end{equation}}\\~\\
Der Zusammaenhang von unendlich langen Folgen von Dirac-Impulsen im Zeit- und Frequenzbereich wird viel bei periodischen Funktionen gebraucht.\\
Weitere Zusammen\-h"ange
zwischen Zeitfunktion und Spektralfunktion sind im
Anhang~\ref{sec:FOU:anhang} aufgef"uhrt. 


